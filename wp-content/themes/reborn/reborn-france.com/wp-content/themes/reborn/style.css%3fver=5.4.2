  }
}



/* -------------------------------------------------------------------------------------------------------------------------------
--------------------------------------------------------Menu Burger------------------------------------------------------------------
---------------------------------------------------------------------------------------------------------------------------------- */

#hamburger,
#hamburger-button {
  display: inline;
}
#hamburger-button {
  display: none;
  cursor: pointer;
  background: none;
  border: none;
  font-size: 30px;
  padding: 0;
  vertical-align: top;
  line-height: 70px;
  float: left;
  margin-left: 30px;
  color: #faf9c4;
}
#hamburger-overlay {
  display: none;
  position: fixed;
  z-index: 10;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.4);
}
#hamburger-sidebar {
  display: none;
  position: fixed;
  width: 80%;
  z-index: 20;
  height: 100%;
  top: 0;
  left: 0;
  background-color: #4f8067;
  transform: translateX(-150%);
  transition: transform 0.2s ease-out;
}

#hamburger-sidebar-body {
  margin-top: 10px;
}

#hamburger-sidebar li {
  display: block;
  padding: 20px 0;
  font-size: 1.5em;
}

#hamburger-sidebar-body > div > nav > ul > li.btn-menu.logo_burger {
  order: -1;
  margin-bottom: 30px;
}

@media screen and (max-width: 720px) {

  .navigation,
  .nav,
  .wrap,
  #hamburger-content {
    display: flex;
    flex-direction: column;
  }

  #hamburger {
    display: flex;
    justify-content: space-between;
  }

  #hamburger-button {
    order: -1;
    margin-bottom: 120px;
  }

  header .logo-header {
    width: 60vw;
    height: auto;
  }

  #element_vide_droite {
    margin-right: 66px;
    order: 2;
  }

  header .active {
    display: none;
  }

  #hamburger-button,
  #hamburger-sidebar,
  .hamburger-activated #hamburger-overlay {
    display: block;
  }
  .hamburger-activated #hamburger-sidebar {
    transform: translateX(0);
  }
}

