/* -------------------------------------------------------------------------------------------------------------------------------
--------------------------------------------------------PAGE NOS PRODUITS------------------------------------------------------------------
---------------------------------------------------------------------------------------------------------------------------------- */
.wrap_produit {
  width: 70%;
  margin: 0 auto;
}

/* ------------------------------------------ Produit Lovidol ----------------------------------------------------------------*/

.produits {
  display: flex;
  margin-top: 100px;
}

.div_deco_bouton_produits {
    margin-bottom: 50px;
    width: 190px;
    height: 50px;
    background-color: #4f8067;
    border: 5px solid #599576;
}

.titre_description_produit_1 {
  margin-right: 10%;
}

.titre_du_produit_1 {
  font-family: westmeathregular, sans-serif;
  color: #ffda89;
  text-transform: uppercase;
  text-shadow: 0 4px 4px rgba(0, 0, 0, 0.25);
  letter-spacing: 0.1em;
  font-size: 2.5vw;
  margin: 50px 0;
}

.description_du_produit_1 {
  font-size: 1.3vw;
}

.cerle_produit_1 {
  display: flex;
  justify-content: center;
  height: 350px;
  width: 350px;
  background-color: #599576;
  border-radius: 50%;
}

.image_du_produit_1 {
  height: auto;
  width: 50%;
  align-self: center;
}

.produit_tronconic {
  flex-direction: row-reverse;
  text-align: right;
    margin-top: 100px;
}

.titre_description_produit_2 {
  margin-left: 10%;
}

.description_du_produit_2 {
  width: 100%;
}

.image_du_produit_2 {
  height: auto;
  width: 65%;
  align-self: center;
  margin-right: 20px;
}

@media screen and (max-width: 1024px) {
  .produits {
    flex-direction: column;
    text-align: center;
  }

  .titre_description_produit_1 {
    margin-right: 0;
  }
  .produit_tronconic {
      margin-top: 50px;
  }
  .titre_description_produit_2 {
    margin-left: 0;
  }
  .titre_du_produit_1 {
    font-size: 5vw;
    margin-top: 50px;
  }
  .description_du_produit_1 {
    font-size: 2.5vw;
  }
  .image_produit_1 {
    margin: 50px auto 0 auto;
  }
  .cerle_produit_1 {
    height: 250px;
    width: 250px;
  }
}

@media screen and (max-width: 720px) {
  .wrap_produit {
    width: 90%;
  }
  .titre_du_produit_1 {
    font-size: 8vw;
  }

  .produits {
    margin-top: 50px;
  }

  .description_du_produit_1 {
    font-size: 4.5vw;
  }
}

/* -------------------------------------------------------------------------------------------------------------------------------
--------------------------------------------------------PAGE NOS TRESORS--------------------------------------------
---------------------------------------------------------------------------------------------------------------------------------- */

.titre_nosTresors {
    margin-left: 0.5%;
    font-size: 3vw;
}

.div_titre_nosTresors {
  margin-top: 100px;
}

.high_bar_nosTresors,
    .low_bar_nosTresors {
        width: 20%;
        margin: 6px auto;
    }

.div_texte_nosTresors {
    width: 90%;
    margin: 50px auto;
}

.texte_nosTresors {
    text-align: center;
}

.img_nosTresors {
    display: flex;
    justify-content: space-around;
    margin: 50px 0;
}

@media screen and (max-width: 1024px) {
    .img_nosTresors {
        flex-direction: column;
        align-items: center;
    }
    .texte_nosTresors {
        font-size: 2vw;
    }
}

@media screen and (max-width: 720px) {
  .div_titre_nosTresors {
    margin-top: 50px;
  }

    .high_bar_nosTresors,
    .low_bar_nosTresors {
        width: 43%;
        margin: 8px auto;
    }
    .titre_nosTresors {
        font-size: 6.5vw;
    }
    .texte_nosTresors {
        /* text-align: center; */
        /* width: 90%; */
        font-size: 4.5vw;
    }
    .img_nosTresors {
        text-align: center;
        /* margin-left: 25%; */
    }
    .image_voiture_nos_tresors img {
        width: 70%;
        height: auto;
        margin-top: 100px;
    }
    .image_machine_a_sous_nos_tresors img {
        width: 50%;
        height: auto;
    }
}

/*--------------------------------------------------------Contact------------------------------------------------------------------
---------------------------------------------------------------------------------------------------------------------------------- */

.nf-field-element {
  text-align: center;
}

.nf-form-content {
  width: 90% !important;
}


.nf-form-content input[type=button]{
  background-color: #4f8067 !important; 
  border: 2px solid #599576 !important;
  color: #faffd1 !important;
  font-size: hansiefregular, sans-serif !important;
}

.nf-form-content input[type=button]:hover {
  border-color: #ffda89 !important;
}

.nf-form-content label {
  color: #faffd1 !important;
}

.nf-form-content input, textarea {
  background-color: #4f8067 !important;
  border: 2px solid #599576 !important;
  color: #faffd1 !important;
}











/* -------------------------------------------------------------------------------------------------------------------------------
--------------------------------------------------------CGU & mentions légales------------------------------------------------------------------
---------------------------------------------------------------------------------------------------------------------------------- */
.titre_principal_cgu_ml {
  font-size: 3vw;
  text-align: left;
}
.wrap_cgu_ml {
  width: 1290px;
  margin: 50px auto;
  max-width: 90%;
  color: #faffd1;
}
.titre_secondaire_cgu_ml {
  font-size: 2.5vw;
}
.liens_reborn_cgu_ml {
  font-size: 1em;
  text-transform: none;
  font-family: thabitregular, sans-serif;
}
.mentions_legales {
  margin: 20px 0;
}
@media screen and (max-width: 720px) {
  .titre_principal_cgu_ml {
      font-size: 5vw;
      text-align: center;
      margin: 25px 0;
  }
  .wrap_cgu_ml p {
      text-align: center;
      font-size: 3vw;
  }
  .titre_secondaire_cgu_ml {
      font-size: 4vw;
      text-align: center;
      margin: 15px 0;
  }
}
@media screen and (min-width: 720px) {
  .titre_principal_cgu_ml {
      margin: 15px 25px;
  }
  .titre_secondaire_cgu_ml {
      margin: 15px 0 15px 50px;
  }
}



/* -------------------------------------------------------------------------------------------------------------------------------
--------------------------------------------------------FOOTER------------------------------------------------------------------
---------------------------------------------------------------------------------------------------------------------------------- */

.site-footer {
  font-family: hansiefregular, sans-serif;
}
.nav_footer {
  display: flex;
  justify-content: space-around;
  /* height: 100%; */
  background-color: #599576;
  flex-flow: row wrap;
  width: 100%;
}
.titre_footer {
  font-size: 25px;
  letter-spacing: 0.1em;
  color: #ffda89;
  margin: 25px 0;
}
.titre_reseaux {
    font-family: hansiefregular, sans-serif;
    margin-top: 20px;
}
.nav_footer ul li a {
  color: #faffd1;
  text-decoration: none;
  line-height: 30px;
  letter-spacing: 0.1em;
  text-transform: uppercase;
  text-shadow: none;
}
.copyright {
  text-transform: uppercase;
  text-align: center;
  margin: 10px 0;
  letter-spacing: 0.1em;
  font-size: 15px;
  color: #faffd1;
}
.reseaux_footer {
    display: flex;
    width: 15%;
    flex-direction: column;
}
.reseaux_footer img {
    width: 10%;
}
.reseauI, .reseauL, .reseauT {
    margin: 10px 0;
}
.reseauF {
    padding-bottom: 10px;
}
@media screen and (max-width: 1024px) {
  .nav_footer {
    height: 400px;
  }
  .reborn_footer,
  .service_client_footer,
  .mentions_legales_footer {
    width: 50%;
    text-align: center;
  }
    .reseaux_footer {
        width: 50%;
        text-align: center;
    }
    .reseaux_footer img {
        width: 5%;
    }
}
@media screen and (max-width: 720px) {
  .nav_footer {
    height: 690px;
  }
  .reborn_footer,
  .service_client_footer,
  .mentions_legales_footer {
    width: 100%;
    text-align: center;
  }
    .reseaux_footer {
        width: 100%;
    }
    .reseaux_footer img {
        width: 4%;
    }
}